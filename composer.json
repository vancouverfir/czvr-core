{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.168",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.24",
        "graham-campbell/markdown": "^16.0",
        "guzzlehttp/guzzle": "^7.0",
        "inertiajs/inertia-laravel": "^0.6.3",
        "intervention/image": "^2.5",
        "laravel-notification-channels/discord": "^1.1",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.5",
        "lasserafn/php-initial-avatar-generator": "^4.0",
        "mofodojodino/profanity-filter": "^1.3",
        "parsedown/laravel": "^1.2",
        "sentry/sentry-laravel": "^4.10",
        "socialiteproviders/discord": "^4.1",
        "spatie/laravel-backup": "^9.2",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-permission": "^6.10",
        "vatsim/sso": "3.*",
        "wohali/oauth2-discord-new": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "spatie/ignition": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
